<Window x:Class="CAMS_ETL.frmCreateDb"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Create New Database" Height="102" Width="417">
    <Grid Margin="0,0,0,2">
        <Label Content="Database Name:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="-5,3,0,0"/>
        <TextBox x:Name="txtDbName" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="285" Margin="114,6,0,0"/>
        <Button x:Name="btnCreate" Content="Create" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="244,34,0,0" Click="btnCreate_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="324,34,0,0" Click="btnCancel_Click"/>

    </Grid>
</Window>
